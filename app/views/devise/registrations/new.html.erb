<section class = "flex justify-center">
  <%= render "devise/shared/error_messages", resource: resource %>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "p-10 text-xl rounded-lg w-30rem bg-white"}) do |f| %>
    <%= render "new", f: f%>
  <% end %>
</section>


